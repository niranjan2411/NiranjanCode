//  <!-- Problem Link: https://www.geeksforgeeks.org/problems/reverse-digits/1  -->

class Solution {
  public:
    int reverseDigits(int n) {
        // Code here
        int num = 0;
        while(n>0){
            int a = n%10;
            n = n/10;
            num = num*10 + a;;
        }
        return num;
    }
};

